$('#<%= dom_id @vote.suggestion %>').replaceWith(<%= render(@vote.suggestion).to_json.html_safe %>);
(function () {
	var savedNotifications = $('#<%= dom_id @vote.suggestion %> span.saved');
	setTimeout(function () {
		savedNotifications.fadeOut('slow', function () { $(this).remove(); });
	}, 3000);
})();
